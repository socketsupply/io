{
  "name": "@socketsupply/io",
  "version": "1.0.10",
  "main": "index.js",
  "scripts": {
    "lint": "standardx -v",
    "test": "tape ./test/node/*.js",
    "prepublish": "esbuild index.js --outfile=index.cjs --platform=node --format=cjs",
    "pub": "npm pub && npm publish --registry https://npm.pkg.github.com"
  },
  "type": "module",
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/socketsupply/mobile.git"
  },
  "bugs": {
    "url": "https://github.com/socketsupply/mobile/issues"
  },
  "homepage": "https://github.com/socketsupply/mobile#readme",
  "description": "javascript sugar on top of socket-sdk ipc",
  "devDependencies": {
    "esbuild": "0.14.49",
    "tapzero": "*"
  },
  "standardx": {
    "ignore": [
      "src/main/logger/vendor/*.js",
      "src/vendor/*.js",
      "test/example-app/dist",
      "index.d.ts"
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "script"
    },
    "rules": {
      "strict": [
        "error",
        "global"
      ]
    }
  },
  "dependencies": {
    "buffer": "6.0.3",
    "streamx": "2.12.4",
    "uuid": "8.3.2"
  }
}
